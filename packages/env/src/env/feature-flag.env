# Environment
NODE_ENV=development

# Server-side flags
ENABLE_NEW_BACKEND_API=false
ENABLE_CACHING=true
MAX_CONCURRENT_REQUESTS=10
ENABLE_RATE_LIMITING=false
LOG_LEVEL=info
USE_LEGACY_DATABASE=false

# Client-side flags
NEXT_PUBLIC_ENABLE_BETA_FEATURES=false
NEXT_PUBLIC_NEW_UI_VERSION=v1

# Authentication
NEXT_PUBLIC_ENABLE_AUTH=true
NEXT_PUBLIC_AUTH_PROVIDERS=email,google

# Pricing and subscription
NEXT_PUBLIC_ENABLE_PRICING=false
NEXT_PUBLIC_ENABLE_SUBSCRIPTION_MANAGEMENT=false

# Analytics
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ANALYTICS_MODULES=merchant,location,realtime

# UI/UX features
NEXT_PUBLIC_ENABLE_DARK_MODE=true
NEXT_PUBLIC_ENABLE_CUSTOMIZABLE_DASHBOARD=false
NEXT_PUBLIC_ENABLE_GUIDED_ONBOARDING=true

# Performance optimizations
NEXT_PUBLIC_ENABLE_LAZY_LOADING=true
NEXT_PUBLIC_ENABLE_SERVICE_WORKER=false

# Smart Goal features
NEXT_PUBLIC_ENABLE_SMART_GOALS=true
NEXT_PUBLIC_SMART_GOAL_FEATURES=milestones,forecasts,notes,progress_tracking

# Milestone features
NEXT_PUBLIC_ENABLE_MILESTONES=true
NEXT_PUBLIC_MILESTONE_FEATURES=completion_tracking,budget_integration

# Forecast features
NEXT_PUBLIC_ENABLE_FORECASTS=true
NEXT_PUBLIC_FORECAST_FEATURES=amount_prediction,completion_date_prediction,variance_calculation

# Feature flag validation
SKIP_FEATURE_FLAG_VALIDATION=false